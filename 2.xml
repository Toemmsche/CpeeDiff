<description xmlns="http://cpee.org/ns/description/1.0">
    <call id="a1" endpoint="http://gruppe.wst.univie.ac.at/~mangler/services/airline.php">
        <parameters>
            <label>Book Airline</label>
            <method>:post</method>
            <arguments>
                <from>data.from</from>
                <to>data.to</to>
                <persons>data.persons</persons>
            </arguments>
        </parameters>
        <code>
            <finalize output="result">data.airline = result.value('id'); data.costs += result.value('costs').to_f; data *= 1.19;</finalize>
        </code>
    </call>
    <manipulate>data.adfsafs =12;</manipulate>
</description>