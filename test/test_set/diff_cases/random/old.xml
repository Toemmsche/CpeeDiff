<description xmlns="http://cpee.org/ns/description/1.0">
    <parallel>
        <parallel_branch>
            <manipulate>data.costs = 1;</manipulate>
        </parallel_branch>
    </parallel>
    <parallel>
        <parallel_branch>
            <call id="a1" endpoint="bookAir">
                <parameters>
                    <method>:post</method>
                    <arguments>
                        <from>data.from</from>
                        <to>data.to</to>
                    </arguments>
                </parameters>
                <code>
                    <finalize output="result">data.airline = result.value('id'); data.costs +=
                        result.value('costs').to_f;
                    </finalize>
                </code>
            </call>
            <manipulate>data.costs *= 1.19;</manipulate>
        </parallel_branch>
        <parallel_branch>
            <terminate/>
        </parallel_branch>
    </parallel>
</description>