{
  "name": "cpeediff",
  "version": "0.1.0",
  "description": "An advanced difference tool for process models conforming to the [CPEE](https://cpee.org) notation.",
  "main": "src/main.js",
  "esversion": 21,
  "scripts": {
    "run": "node src/main.js"
  },
  "bin": {
    "cpeediff": "src/main.js"
  },
  "type": "module",
  "keywords": [
    "CPEE",
    "cpee.org",
    "Process trees",
    "Diff",
    "Tree Diff",
    "XML",
    "Semantic Diff"
  ],
  "author": "Tom Papke",
  "license": "Apache 2.0",
  "dependencies": {
    "markdown-table": "^3.0.1",
    "murmur-32": "^0.2.0",
    "node": "^16.4.0",
    "vkbeautify": "^0.99.3",
    "@xmldom/xmldom": "^0.7.4",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.15.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": "google",
    "rules": {},
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2021,
      "babelOptions": {
        "configFile": "./.babelrc"
      }
    },
    "parser": "@babel/eslint-parser"
  }
